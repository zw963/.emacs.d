AppBar(
      title: Container(child: const Text('${1:Title}')),
      actions: [
        TextButton(
          onPressed: () async {
            await Application.router.navigateTo(
              context,
              ${2:Page}.routerPath,
            );
            setState(() {});
          },
          child: const Text('${3:Button Title}'),
        )
      ]
      )